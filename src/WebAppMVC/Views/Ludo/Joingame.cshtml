@model WebAppMVC.Models.GameList
@{
    ViewData["Title"] = "Joingame";
    Layout = "_HomeLayout";
}

<div class="text-center">
    <label> Select Game ID: </label>
    <select>
        @for (int i = 0; i < Model.AllGames.Count(); i++)
        {
            <option value="@i">@Model.AllGames.ElementAt(i).Key</option>
        }
    </select>
</div>
<button>Join Game!</button>